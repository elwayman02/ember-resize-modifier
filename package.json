{
  "private": true,
  "repository": {
    "type": "git",
    "url": "git@github.com:elwayman02/ember-resize-modifier.git"
  },
  "license": "MIT",
  "author": "Jordan Hawker <hawker.jordan@gmail.com> (http://www.JordanHawker.com)",
  "scripts": {
    "build": "pnpm --filter ember-resize-modifier build",
    "lint": "pnpm --filter '*' lint",
    "lint:fix": "pnpm --filter '*' lint:fix",
    "prepare": "pnpm build",
    "start": "concurrently 'pnpm:start:*' --restart-after 5000 --prefix-colors cyan,white,yellow",
    "start:addon": "pnpm --filter ember-resize-modifier start --no-watch.clearScreen",
    "start:docs-app": "pnpm --filter docs-app start",
    "start:test-app": "pnpm --filter test-app start",
    "test": "pnpm --filter '*' test",
    "test:ember": "pnpm --filter '*' test:ember"
  },
  "devDependencies": {
    "concurrently": "^9.0.1",
    "prettier": "^3.0.3",
    "prettier-plugin-ember-template-tag": "^2.0.2",
    "release-plan": "^0.17.4"
  },
  "packageManager": "pnpm@9.5.0",
  "pnpm": {
    "overrides": {
      "@types/eslint": "^7.0.0"
    }
  }
}
